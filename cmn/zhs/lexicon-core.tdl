    SYNSEM.LOCAL.CAT.HEAD.CHAR [ FCHAR "开", LENGTH one ],
    SYNSEM [ LOCAL [ CAT.HEAD.LZG le+zhe+guo,
		     CAT.VAL.COMPS < [ OPT - ] > ],
	     LKEYS [ --COMPKEY 会_n_selected_rel,   
		     KEYREL.PRED "_开_v_会_sep_1_rel"] ],
    TRAITS native_token_list ].

会_sep_n_1 := n_nosem_selected &
  [ STEM < "会" >,
    SYNSEM.LOCAL.CAT.HEAD.CHAR [ FCHAR "会", LENGTH one ],
    SYNSEM [ LOCAL.CONT.HOOK.INDEX.PNG.ANIMACY inanimate,
             LKEYS.KEYREL.PRED 会_n_selected_rel ],
    TRAITS native_token_list ].


